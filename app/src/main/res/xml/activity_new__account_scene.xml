<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            motion:layout_constraintEnd_toEndOf="@+id/editTextTextPassword2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/editTextTextPassword2"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintStart_toStartOf="@+id/editTextTextPassword2"
            android:layout_marginTop="10dp"
            android:id="@+id/textView13" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            motion:layout_constraintEnd_toEndOf="@+id/editTextTextPersonName"
            android:layout_width="305dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="12dp"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="378dp"
            android:id="@+id/textView12" />
    </ConstraintSet>
</MotionScene>